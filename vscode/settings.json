{

	"editor.fontFamily": "'Hack Nerd Font Mono', 'FontAwesome', 'monospace'",
	"editor.tabSize": 2,
	"editor.insertSpaces": false,
	"editor.cursorBlinking": "expand",
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "selection",
	"editor.lineNumbers": "interval",
	"editor.acceptSuggestionOnEnter": "off",
	"editor.suggestSelection": "first",
	"editor.multiCursorModifier": "ctrlCmd",
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},

	"explorer.openEditors.visible": 0,
	"explorer.autoReveal": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.confirmDelete": false,

	"window.titleBarStyle": "native",
	"window.restoreWindows": "none",
	"window.zoomLevel": 1,

	"workbench.statusBar.visible": false,
	"workbench.editor.showTabs": false,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.colorTheme": "Tomorrow Night",

	"files.trimTrailingWhitespace": true,
	"files.trimFinalNewlines": true,
	"files.hotExit": "off",
	"files.associations": {
		"*.css": "postcss"
	},
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true,
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/tmp": true,
		"**/node_modules": true,
		"**/bower_components": true,
		"**/dist": true
	},
	"files.watcherExclude": {
		"**/.git/objects": true,
		"**/.git/subtree-cache": true,
		"**/node_modules": true,
		"**/tmp": true,
		"**/bower_components": true,
		"**/dist": true
	},

	"git.confirmSync": false,
	"git.autofetch": true,
	"git.enableSmartCommit": true,
	"git.ignoreRebaseWarning": true,

	"terminal.integrated.copyOnSelection": true,
	"search.exclude": {
		"**/node_modules": true,
		"**/archive": true,
		"**/public": true,
		"**/dist": true
	},

	"customizeUI.font.monospace": "Hack Nerd Font Mono",
	"customizeUI.font.regular": "Hack Nerd Font Mono",
	"customizeUI.moveStatusbar": true,

	"color-highlight.markerType": "underline",
	"color-highlight.markRuler": false,

	"material-icon-theme.hidesExplorerArrows": true,
	"material-icon-theme.showUpdateMessage": false,
	"material-icon-theme.folders.color": "#666",

	"javascript.updateImportsOnFileMove.enabled": "always",
	"diffEditor.ignoreTrimWhitespace": true,
	"disable-hardware-acceleration": true,
	"security.workspace.trust.untrustedFiles": "open",

