bindsym $mod+XF86PowerOff       exec sudo ~/.scripts/system reboot
bindsym $mod+Shift+XF86PowerOff exec sudo ~/.scripts/system shutdown

bindsym $mod+o                  $overwatch

bindsym $mod+Control+u          exec (cd ~/Projects/dotfiles/i3 && ./SETUP)
bindsym $mod+Shift+Control+u    exec (cd ~/Projects/dotfiles/$(cat ~/.config/device) && ./SETUP)