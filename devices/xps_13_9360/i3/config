for_window [class="Pavucontrol"] $overwatch


bindsym $mod+XF86PowerOff       exec sudo /home/chase/.scripts/system reboot
bindsym $mod+Shift+XF86PowerOff exec sudo /home/chase/.scripts/system shutdown

bindsym $mod+o $overwatch

bindsym $mod+Control+u exec ~/dotfiles/i3/update

bindsym $mod+Control+n exec notify-send -u low 'Hello world!' 'This is an example notification.'